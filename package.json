{
  "version": "0.4.2",
  "description": "Uncompromisingly Flexible Phylogenetic Trees, Powered by D3.js",
  "name": "tidytree",
  "main": "dist/tidytree.js",
  "scripts": {
    "all": "npm run build && npm run minify && npm run doc",
    "build": "rollup -c",
    "demo": "npx http-server ..",
    "dev": "npm run demo & npm run watch",
    "doc": "documentation build src/main.js -f md -o docs/README.md",
    "minify": "terser dist/tidytree.js --compress > dist/tidytree.min.js",
    "version": "npm version patch",
    "watch": "rollup -c --watch"
  },
  "author": "Tony Boyles <nsp3@cdc.gov>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaboyles/TidyTree.git"
  },
  "dependencies": {
    "d3": "^5.12.0",
    "patristic": "^0.5.4"
  },
  "devDependencies": {
    "diff": ">=3.5.0",
    "documentation": "^9.3.1",
    "rollup": "^1.21.2",
    "rollup-plugin-node-resolve": "^4.2.4",
    "terser": "^3.17.0"
  }
}
