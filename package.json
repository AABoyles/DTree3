{
  "version": "0.2.5",
  "description": "Yet another d3-powered Phylogenetic tree renderer.",
  "name": "tidytree",
  "main": "dist/tidytree.js",
  "scripts": {
    "build": "rollup -c",
    "demo": "http-server",
    "dev": "npm run demo & npm run watch",
    "doc": "documentation build main.js -f md -o docs/README.md",
    "minify": "terser dist/tidytree.js --compress > dist/tidytree.min.js",
    "watch": "rollup -c --watch"
  },
  "author": "Tony Boyles <nsp3@cdc.gov>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaboyles/TidyTree.git"
  },
  "dependencies": {
    "d3": "^5.9.1",
    "patristic": "^0.2.10"
  },
  "devDependencies": {
    "documentation": "^9.1.1",
    "http-server": "^0.11.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "terser": "^3.16.1"
  }
}
